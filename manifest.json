
    {
      "manifest_version": 2,
      "name": "Twitter Logo Changer",
      "description": "This extension replaces the Twitter logo with a custom image.",
      "version": "1.0",
      "content_scripts": [
        {
          "matches": ["https://twitter.com/*"],
          "js": ["content.js"]
        }
      ],
      "permissions": ["activeTab"]
    }
    
